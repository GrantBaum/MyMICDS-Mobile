  "scripts": {
    "build": "mjml -r src/email/* -o src/html/messages",
    "start": "npm run build && node dist/index.js",
    "prod": "npm run build && pm2 start dist/index.js -i max --watch=\"src\" --ignore-watch=\"src/public src/api\" --name=\"mymicds\"",
    "tasks": "pm2 start dist/tasks.js --watch=\"src\" --ignore-watch=\"src/public src/api\" --name=\"mymicds-tasks\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts": "tsc",
    "ts:watch": "tsc -w",
    "docs": "typedoc --out ./docs --mode file ./src",
    "lint": "tslint --project tsconfig.json --exclude \"src/libs/config.js\" \"src/**/*.ts\""
  },
